	<span class="nb">set </span>selectedText to first item of input
	<span class="nb">set </span>wordName to second item of input
	tell application <span class="s2">"Evernote"</span>
		<span class="k">if</span> <span class="o">(</span>not <span class="o">(</span>notebook named <span class="s2">"wordlist"</span> exists<span class="o">))</span> <span class="k">then
